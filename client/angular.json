            "dev": {
              "protractorConfig": "projects/virkailija-app-e2e/protractor.conf.js",
              "devServerTarget": "virkailija-app:serve"
            },
            "prod": {
              "protractorConfig": "projects/virkailija-app-e2e/protractor-prod.conf.js",
